<data-settings>

  <!-- Defines if should read real data (true) of simulate data (false) -->
  <data>true</data>


  <!-- In case of real data -->
  <real-data>
    <folder-path>data/</folder-path>
    <group-file>group.csv</group-file>
    <timepoints-file>X.csv</timepoints-file>

    <!-- Type of data to read : if nothing, then the file is not read -->
    <observations>

      <!-- Cognitive Scores attributes -->
      <cognitive-scores>
          <presence>yes</presence>
          <path-to-data>Y.csv</path-to-data>
          <dimension>4</dimension>
      </cognitive-scores>

      <!-- Landmarks attributes -->
      <landmarks>
          <presence>no</presence>
          <path-to-data>Y.csv</path-to-data>
          <dimension>1827</dimension>
      </landmarks>

    </observations>

  </real-data>

  <!-- In case of simulated data -->
  <simulated-data>
    <number-of-individuals>150</number-of-individuals>
    <min-number-of-observations>4</min-number-of-observations>
    <max-number-of-observations>6</max-number-of-observations>
  </simulated-data>


</data-settings>
