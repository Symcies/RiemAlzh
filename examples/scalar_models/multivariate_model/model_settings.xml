<model-settings>

  <!-- Type of the model : {Univariate, Multivariate, FastNetwork, MixedEffect} -->
  <type>Multivariate</type>

  <!-- Name of the outputs files -->
  <output-name>AwesomeNetwork</output-name>

  <number-of-independent-sources>3</number-of-independent-sources>

  <variables>
    <noise>
      <name>noise</name>
      <initial-parameters>
        <mean>0.0</mean>
        <variance>0.00001</variance>
      </initial-parameters>
      <second-parameters>
        <mean>0.0</mean>
        <variance>0.001</variance>
      </second-parameters>
      <proposition-variance>0</proposition-variance>
    </noise>

    <G>
      <name>G</name>
      <initial-parameters>
        <mean>0.12</mean>
        <variance>0.00000001</variance>
      </initial-parameters>
      <second-parameters>
        <mean>0.10</mean>
        <variance>0.00001</variance>
      </second-parameters>
      <proposition-variance>0.00001</proposition-variance>
    </G>

    <Delta>
      <name>Delta</name>
      <initial-parameters>
        <mean>0</mean>
        <variance>0.000009</variance>
      </initial-parameters>
      <second-parameters>
        <mean>0.0002</mean>
        <variance>0.001</variance>
      </second-parameters>
      <proposition-variance>0.0001</proposition-variance>
    </Delta>

    <Beta>
      <name>Beta</name>
      <initial-parameters>
        <mean>0</mean>
        <variance>0.000001</variance>
      </initial-parameters>
      <second-parameters>
        <mean>0.0001</mean>
        <variance>0.001</variance>
      </second-parameters>
      <proposition-variance>0.0001</proposition-variance>
    </Beta>


    <Ksi>
      <name>Ksi</name>
      <initial-parameters>
        <mean>0.13</mean>
        <variance>0.0004</variance>
      </initial-parameters>
      <second-parameters>
        <mean>0.10</mean>
        <variance>0.000004</variance>
      </second-parameters>
      <proposition-variance>0.0001</proposition-variance>
    </Ksi>

    <Tau>
      <name>Tau</name>
      <initial-parameters>
        <mean>75</mean>
        <variance>0.025</variance>
      </initial-parameters>
      <second-parameters>
        <mean>70</mean>
        <variance>25</variance>
      </second-parameters>
      <proposition-variance>0.5</proposition-variance>
    </Tau>


    <S>
      <name>S</name>
      <initial-parameters>
        <mean>0</mean>
        <variance>1</variance>
      </initial-parameters>
      <second-parameters>
        <mean>85</mean>
        <variance>25</variance>
      </second-parameters>
      <proposition-variance>0.7</proposition-variance>
    </S>

  </variables>

  <acceptance-ratio-to-display>
    <G>G</G>
    <ksi>Ksi</ksi>
    <tau>Tau</tau>
  </acceptance-ratio-to-display>

</model-settings>
